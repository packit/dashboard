{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@patternfly/react-charts": "^6.94.19",
    "@patternfly/react-core": "^4.276.8",
    "@patternfly/react-icons": "^4.93.6",
    "@patternfly/react-log-viewer": "^4.87.100",
    "@patternfly/react-styles": "^4.92.6",
    "@patternfly/react-table": "^4.113.0",
    "@tanstack/react-query": "^4.29.7",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.43.0",
    "msw": "^1.2.1",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.14.0",
    "react-router-dom": "^6.6.2",
    "typescript": "^5.0.2",
    "vite": "^4.2.3",
    "vite-bundle-visualizer": "^0.7.0",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "proxy": "http://127.0.0.1:5000",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:analyze": "vite-bundle-visualizer --open",
    "preview": "vite preview",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "dist/*",
      "public/*",
      "frontend/*"
    ],
    "extends": [
      "plugin:storybook/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          "*.(js|jsx|ts|tsx)"
        ],
        "options": {
          "tabWidth": 4
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.0.9",
    "@storybook/addon-actions": "^7.0.9",
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/blocks": "^7.0.18",
    "@storybook/builder-vite": "^7.0.18",
    "@storybook/components": "^7.4.0",
    "@storybook/core-events": "^7.4.0",
    "@storybook/manager-api": "^7.4.0",
    "@storybook/node-logger": "^7.0.9",
    "@storybook/preview-api": "^7.4.0",
    "@storybook/react": "^7.0.9",
    "@storybook/react-vite": "^7.0.18",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.4.0",
    "@storybook/types": "^7.4.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-plugin-storybook": "^0.6.12",
    "msw-storybook-addon": "^1.8.0",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.9",
    "storybook-addon-react-router-v6": "^1.0.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
